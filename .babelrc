{
    "presets": [["next/babel"]],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "@components": "./components",
            "@styles": "./styles"
          }
        }
      ],
      ["styled-components", { "ssr": true, "displayName": true, "preprocess": false }]
    ]
  }
  